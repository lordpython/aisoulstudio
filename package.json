{
  "name": "aisoulstudio-workspace",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @studio/frontend dev",
    "dev:host": "pnpm --filter @studio/frontend dev:host",
    "server": "pnpm --filter @studio/server dev",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run server\"",
    "dev:all:host": "concurrently \"pnpm run dev:host\" \"pnpm run server\"",
    "build": "pnpm --filter @studio/frontend build",
    "preview": "pnpm --filter @studio/frontend preview",
    "test": "pnpm --filter @studio/frontend test",
    "test:ui": "pnpm --filter @studio/frontend exec vitest --ui",
    "test:run": "pnpm --filter @studio/frontend test:run",
    "test:server": "vitest run --config vitest.config.ts",
    "test:e2e": "playwright test",
    "test:agent": "npx tsx scripts/test-agent-node.ts",
    "test:pipeline": "npx tsx scripts/test-full-pipeline.ts",
    "test:langchain": "npx tsx scripts/test-agent-langchain.ts",
    "codemaps": "pnpm run codemaps:export && pnpm run codemaps:deps",
    "codemaps:export": "npx repomix --config repomix.config.json",
    "codemaps:deps": "npx depcruise --config .dependency-cruiser.cjs -T html -f codebase-map.html packages",
    "generate:sitemap": "npx tsx scripts/generate-sitemap.ts",
    "generate:sitemap:auto": "npx tsx scripts/auto-generate-sitemap.ts",
    "cap:sync": "npx cap sync",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios",
    "build:mobile": "pnpm run build && npx cap sync"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dependency-cruiser": "^17.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
